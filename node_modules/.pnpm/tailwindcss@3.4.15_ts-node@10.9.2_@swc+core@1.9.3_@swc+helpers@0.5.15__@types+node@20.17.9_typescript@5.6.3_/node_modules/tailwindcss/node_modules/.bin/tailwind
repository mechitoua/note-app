#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mehmet/Documents/Projects/note-app/node_modules/.pnpm/tailwindcss@3.4.15_ts-node@10.9.2_@swc+core@1.9.3_@swc+helpers@0.5.15__@types+node@20.17.9_typescript@5.6.3_/node_modules/tailwindcss/lib/node_modules:/home/mehmet/Documents/Projects/note-app/node_modules/.pnpm/tailwindcss@3.4.15_ts-node@10.9.2_@swc+core@1.9.3_@swc+helpers@0.5.15__@types+node@20.17.9_typescript@5.6.3_/node_modules/tailwindcss/node_modules:/home/mehmet/Documents/Projects/note-app/node_modules/.pnpm/tailwindcss@3.4.15_ts-node@10.9.2_@swc+core@1.9.3_@swc+helpers@0.5.15__@types+node@20.17.9_typescript@5.6.3_/node_modules:/home/mehmet/Documents/Projects/note-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mehmet/Documents/Projects/note-app/node_modules/.pnpm/tailwindcss@3.4.15_ts-node@10.9.2_@swc+core@1.9.3_@swc+helpers@0.5.15__@types+node@20.17.9_typescript@5.6.3_/node_modules/tailwindcss/lib/node_modules:/home/mehmet/Documents/Projects/note-app/node_modules/.pnpm/tailwindcss@3.4.15_ts-node@10.9.2_@swc+core@1.9.3_@swc+helpers@0.5.15__@types+node@20.17.9_typescript@5.6.3_/node_modules/tailwindcss/node_modules:/home/mehmet/Documents/Projects/note-app/node_modules/.pnpm/tailwindcss@3.4.15_ts-node@10.9.2_@swc+core@1.9.3_@swc+helpers@0.5.15__@types+node@20.17.9_typescript@5.6.3_/node_modules:/home/mehmet/Documents/Projects/note-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli.js" "$@"
else
  exec node  "$basedir/../../lib/cli.js" "$@"
fi
